+log() { echo -e "[\033[34m$(date +"%H:%M:%S")\033[0m] $*" | tee -a "$LOG_FILE"; }
+log_success() { echo -e "[\033[34m$(date +"%H:%M:%S")\033[0m] \033[32m‚úì\033[0m $*" | tee -a "$LOG_FILE"; }
